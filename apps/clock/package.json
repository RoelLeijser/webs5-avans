{
  "name": "@webs5/clock",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest --detectOpenHandles",
    "preinstall": "npx only-allow pnpm",
    "db:push": "dotenv -e  ../../.env -- npx prisma db push",
    "db:studio": "dotenv -e  ../../.env -- npx prisma studio",
    "db:generate": "dotenv -e  ../../.env -- npx prisma generate"
  },
  "jest": {
    "preset": "@webs5/jest-presets/jest/node"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.9.2",
    "bull": "^4.12.2",
    "rabbitmq-client": "^4.5.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@webs5/eslint-config": "workspace:*",
    "@webs5/jest-presets": "workspace:*",
    "@webs5/typescript-config": "workspace:*",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
