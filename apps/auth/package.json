{
  "name": "@webs5/auth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "prisma generate",
    "start": "node dist/index.js",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "build": "tsup",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest --detectOpenHandles",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "prisma generate",
    "db:push": "dotenv -e  ../../.env -- npx prisma db push",
    "db:studio": "dotenv -e  ../../.env -- npx prisma studio",
    "db:generate": "dotenv -e  ../../.env -- npx prisma generate"
  },
  "jest": {
    "preset": "@webs5/jest-presets/jest/node"
  },
  "dependencies": {
    "@casl/ability": "^6.7.0",
    "@casl/prisma": "^1.4.1",
    "@prisma/client": "^5.10.2",
    "@t3-oss/env-core": "^0.9.2",
    "argon2": "^0.40.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "rabbitmq-client": "^4.5.1",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.6.13",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@webs5/eslint-config": "workspace:*",
    "@webs5/jest-presets": "workspace:*",
    "@webs5/typescript-config": "workspace:*",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.7.0",
    "prisma": "^5.10.2",
    "supertest": "^6.3.4",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
